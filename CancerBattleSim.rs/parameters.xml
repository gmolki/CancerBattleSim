<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="cells_ratio" displayName="Ratio NKCells/CCells" type="float" 
						defaultValue="8.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="nkcell_count" displayName="Count NKCell" type="int" 
						defaultValue="3200" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="weight_calculation" displayName="Weight Calculation" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									values="0 1 "
						
		/>
				<parameter name="ccell_count" displayName="Count CCell" type="int" 
						defaultValue="400" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="ncell_count" displayName="Count NCell" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


